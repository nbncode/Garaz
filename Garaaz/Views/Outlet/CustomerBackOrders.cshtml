@model List < Garaaz.Models.BackOrderModel>
@{
    ViewBag.Title = "CustomerBackOrders";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="kt-grid__item kt-grid__item--fluid kt-grid kt-grid--hor">
    <div class="kt-subheader-search">
        <h3 class="kt-subheader-search__title">
            @ViewBag.Title
        </h3>
    </div>
    <header class="ribbon">
        <h2>&nbsp;</h2>
    </header>
    <div class="page-content">
        <div class="page-content">
            <div class="kt-content  kt-grid__item kt-grid__item--fluid" id="main-content">
                @{Html.RenderPartial("alerts");}
                <div class="row">
                    <div class="col-md-12">
                        <!--begin::Portlet-->
                        <div class="kt-portlet">
                            <div class="kt-portlet__head">
                                <div class="kt-portlet__head-label">
                                    <h3 class="kt-portlet__head-title">
                                        @ViewBag.Title
                                    </h3>
                                </div>
                            </div>
                            <div class="panel-body">
                                <div class="kt-form">
                                    <div class="kt-portlet__body">
                                        <div class="row">
                                            <div class="col-sm-12 text-right" style="margin: 0 0 15px;">
                                                <br />
                                                <input type="submit" class="btn btn-info" onclick="admin.OutletCboExportToExel()" value="Export to Excel">
                                            </div>
                                        </div>

                                        <div class="row backOrders">
                                            @{Html.RenderPartial("_BackOrders", Model);}
                                        </div>
                                    </div>
                                </div>
                                <!-- /.panel-primary panel -->
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

